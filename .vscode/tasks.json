{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "buildDepuracion",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Depuracion/Depuracion.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildExceptions",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Exceptions/Exceptions.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}